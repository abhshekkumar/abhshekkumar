abstract class Money{  
abstract void rateofinterest();  
}  
//In real scenario, implementation is provided by others i.e. unknown by end user  
class sbi extends Money{  
void rateofinterest(){System.out.println("74%");}  
}  
class sbm extends Money{  
void rateofinterest(){System.out.println("55%");}  
}  
//In real scenario, method is called by programmer or user  
class TestAbstraction1{  
public static void main(String args[]){  
Money s=new sbi();//In a real scenario, object is provided through method, e.g., getShape() method  
s.rateofinterest();
}
}